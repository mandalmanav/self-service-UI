    

.formfield{
    display: flex;
    flex-direction: column;
    text-align: left;
    margin: 10px auto;
    /* width: 300px; */
}
.formfield label{
   font-size: 21px;
    color: #2257ce;
    position: relative;
    margin-bottom: 0;
}
.formfield label span{
    right: 5px;
    cursor: pointer;
    font-size: 18px;
    color: #C4CDD5;
    position: absolute;    
}
.formfield .totaltext{
    font-size: 15px;
    color: #909090;
}
.formfield .totaltext span{
    color: #348CFF;  
}
.formfield .ant-select-selection--single{
    height: 35px;
    position: relative;
    cursor: pointer;
    background: transparent;
}

.exploreFileUploader{
    width: 100%;    
    border: none;
    margin: 20px auto;
    background: transparent;
}

.exploreFileUploader .ant-upload.ant-upload-drag{
    padding: 10px;
}
