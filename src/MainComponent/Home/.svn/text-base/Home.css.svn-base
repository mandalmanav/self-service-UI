.createbttn-section{    
    text-align:center;
    padding: 10px 15px;
    /* background:#eaeaea; */
    /* border-radius: 0px 0px 12px 12px; */
}
.createbttn-section a{    
      font-size: 15px;
    font-weight: 500;
    color: #2257ce !important;
    padding: 6px 23px;
    border: 1px solid #8e8e8e;
    border-radius: 50px;
}
.createbttn-section a:hover{
    border: 1px solid #2257CE
}

.createbttn-section span{
    float:right;
    color:#C4CDD5;
    margin-right:10px;
}
.createbttn-section span i{
    cursor: pointer;
    font-size: 20px;
    margin-right: 20px;    
}
.createbttn-section span i:last-child{
    margin-right: 0;
}
.explore-page h2{
    margin: 30px;
    font-size: 30px;
    color: #3E3ECC;
    font-weight: normal;
}



