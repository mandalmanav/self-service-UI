.explorebtn{
   /* width: 300px;  */
    margin: 0 auto;
    text-align: center;
    /* background: #eaeaea; */
    display: flex;
    /* height: 100px; */
    flex-direction: column;
}
.explorebtn span {
    display: block;
    margin-bottom: 20px;
    font-weight: 600;
}

.explorebtn .ant-btn{
    line-height: 50px;
    background: #2257CE;
    height: 50px;
    color: #fff;
    padding: 0 70px;
    font-weight: normal;
    border-radius: 50px;
    border:none
}

.btn-ss-theme{
     line-height: 50px;
    background: #2257CE;
    height: 50px;
    color: #fff;
    padding: 0 70px;
    font-weight: normal;
    border-radius: 50px;
    border:none;
    cursor: pointer;
}
.pt-ctreated-text span{
display: block;
font-size: 12px;
}
.pt-ctreated-text label{
    font-size: 12px;
    color: black;
}
.explore-page .ant-card-body{
    padding: 0px;
    text-align: center;
    background: white;
    border-radius: 6px;
    box-shadow: 0px 2px 6px rgba(0, 0, 0, 0.1)
}
.explore-page .ant-row{
    padding: 15px;
}
.no-metadata{
    padding: 40px;
    text-align: center;
    background: white;
    border-radius: 12px;
}
.no-metadata span{
    font-size: 15px;
    color: #C4CDD5;
    font-weight: normal;
}
.no-metadata .borderbttm{
    border-bottom:2px solid red;
}
.no-metadata p{
    margin-top: 20px;
    color: #6b6b6b;
    font-style: italic;
    font-size: 15px;
}
